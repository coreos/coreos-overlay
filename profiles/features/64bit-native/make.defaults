# We still need lib to be a symlink since LIBDIR is lib64
SYMLINK_LIB="yes"

USE="-gcc64"
STAGE1_USE="nptl nptlonly unicode"

# Since we're doing a pure 64-bit, we want LIBDIR to be lib64
LIBDIR_amd64="lib64"
LIBDIR_ppc64="lib64"

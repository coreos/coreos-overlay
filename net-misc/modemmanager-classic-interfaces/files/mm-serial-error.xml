  <tp:error name="SerialOpenFailed">
    <tp:docstring>
      Could not open serial device.
    </tp:docstring>
  </tp:error>

  <tp:error name="SerialSendFailed">
    <tp:docstring>
      Could not write to the serial device.
    </tp:docstring>
  </tp:error>

